{
  "manifest_version": 3,
  "name": "MyAgent",
  "version": "0.1",

  "description": "Personal all-in-one Chrome externsion",
  "author": "ylxdzsw@gmail.com",

  "icons": {
    "100": "icon.png"
  },

  "content_scripts": [{
    "matches": ["*://*.github.com/*"],
    "css": ["github.css"]
  }, {
    "matches": ["https://pmotschmann.github.io/Evolve/"],
    "css": ["evolve.css"]
  }, {
    "matches": ["https://www.coursera.org/learn/*"],
    "css": ["coursera.css"],
    "js": ["audio-amplification.js"],
    "run_at": "document_end"
  }]
}
